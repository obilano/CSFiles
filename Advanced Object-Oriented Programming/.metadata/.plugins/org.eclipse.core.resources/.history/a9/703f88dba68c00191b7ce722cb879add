import java.util.*;

public class EliminateDuplicates {

	public static void main(String[] args) {
		int [] array = null ;
		Scanner input = new Scanner(System.in);
		System.out.print("Enter 10 integers: ");
		for(int i = 0; i < 10; i++) {
			array[i] = input.nextInt();
			System.out.print(array[i]);
		}
		
		input.close();
	}
	
}
